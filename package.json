{
  "name": "ai-sdk-provider-codex-app-server",
  "version": "1.0.2",
  "description": "Vercel AI SDK provider for OpenAI Codex using app-server mode with mid-execution injection support",
  "license": "MIT",
  "author": "Pablo Fernandez",
  "homepage": "https://github.com/pablof7z/ai-sdk-provider-codex-app-server",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pablof7z/ai-sdk-provider-codex-app-server.git"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@ai-sdk/provider": "^3.0.2",
    "@ai-sdk/provider-utils": "^2.2.0",
    "zod": "^3.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "ai": "^6.0.28",
    "tsup": "^8.5.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "zod": "^3.0.0 || ^4.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "ai",
    "ai-sdk",
    "openai",
    "codex",
    "vercel",
    "provider",
    "llm",
    "agent"
  ]
}
